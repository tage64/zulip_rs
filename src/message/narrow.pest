WHITESPACE = _{ WHITE_SPACE }
ESCAPED_CHAR = @{ "\\" ~ ANY }
QUOTED_STRING = ${"\"" ~ QUOTED_STRING_CONTENT ~ "\"" }
  QUOTED_STRING_CONTENT = { (!("\"") ~ (ESCAPED_CHAR | ANY))* }
UNQUOTED_STRING = @{ (!(":" | "\"" | WHITESPACE) ~ (ESCAPED_CHAR | ANY))+ }
STRING = { QUOTED_STRING | UNQUOTED_STRING }
NEGATION = { "-" }
FILTER = { NEGATION? ~ STRING ~ ":" ~ STRING }
FILTERS = { FILTER* }
KEYWORDS = { STRING* }
NARROW = { SOI ~ FILTERS ~ KEYWORDS ~ EOI }
